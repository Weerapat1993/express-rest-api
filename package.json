{
  "name": "kunai-express",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "nodemon -e js,graphql -x babel-node ./bin/www",
    "start": "nodemon -e js,graphql -x babel-node ./bin/www",
    "migrate": "knex migrate:latest",
    "make:migration": "knex migrate:make",
    "migrate:seed": "npm run migrate && npm run db:seed",
    "migrate:rollback": "knex migrate:rollback",
    "migrate:reset": "knex migrate:rollback",
    "migrate:refresh": "npm run migrate:reset && npm run migrate && npm run db:seed",
    "make:seeder": "knex seed:make",
    "db:seed": "knex seed:run",
    "apidoc": "apidoc -i app/Http/Controllers -o public"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "bookshelf": "^0.13.3",
    "bookshelf-modelbase": "^2.10.4",
    "chalk": "^2.3.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.2.0",
    "dotenv": "^5.0.1",
    "express": "~4.14.0",
    "express-graphql": "^0.6.12",
    "faker": "^4.1.0",
    "fs-extra": "^7.0.0",
    "graphql": "^0.13.2",
    "graphql-tools": "^3.1.1",
    "graphql-yoga": "^1.16.1",
    "jade": "~1.11.0",
    "joi": "^14.0.4",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "knex": "^0.15.2",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "mysql": "^2.16.0",
    "object-hash": "^1.3.0",
    "passport": "^0.4.0",
    "passport-facebook": "^2.1.1",
    "passport-jwt": "^4.0.0",
    "prisma-binding": "^2.1.4",
    "querystring": "^0.2.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "^1.17.2"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "bin/*"
    ],
    "delay": "2500"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "import/prefer-default-export": 0
    }
  }
}
