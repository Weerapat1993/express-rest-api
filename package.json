{
  "name": "kunai-express",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "npm run apidoc && nodemon --exec babel-node ./bin/www",
    "apidoc": "apidoc -i app/Http/Controllers -o public"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "chalk": "^2.3.2",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.2.0",
    "dotenv": "^5.0.1",
    "express": "~4.14.0",
    "faker": "^4.1.0",
    "jade": "~1.11.0",
    "lodash": "^4.17.5",
    "morgan": "^1.9.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "^1.17.2"
  },
  "nodemonConfig": {
    "ignore": [
      "public/*",
      "bin/*"
    ],
    "delay": "2500"
  },
  "eslintConfig": {
    "extends": "airbnb",
    "rules": {
      "import/prefer-default-export": 0
    }
  }
}
